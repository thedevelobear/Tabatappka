    "lint:source": "eslint 'src/**/*.{tsx,ts}' --max-warnings=0",
    "lint:staged": "lint-staged",
    "prettier": "prettier",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "test": "vitest"
